{
  "properties": [
    {
      "name": "jwt.expire-time",
      "type": "java.lang.Long",
      "description": "jwt令牌过期时间.",
      "defaultValue": 86400
    },
    {
      "name": "jwt.path.include",
      "type": "java.lang.String[]",
      "defaultValue": [
        "/**"
      ],
      "description": "jwt验证拦截路径."
    },
    {
      "name": "jwt.path.exclude",
      "type": "java.lang.String[]",
      "defaultValue": [],
      "description": "jwt验证排除路径."
    },
    {
      "name": "jwt.secret-key",
      "type": "java.lang.String",
      "defaultValue": "123456",
      "description": "secret key for jwt hash algo."
    },
    {
      "name": "jwt.www.header",
      "type": "java.lang.String",
      "defaultValue": "Authentication",
      "description": "."
    },
    {
      "name": "jwt.www.type",
      "type": "java.lang.String",
      "defaultValue": "Bearer",
      "description": "."
    }
  ]
}